<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SampleApplication.Resources"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="{x:Static local:ResourceKeys.DefaultKey}">Default</system:String>
    <system:String x:Key="{x:Static local:ResourceKeys.RuRuKey}">ru-RU</system:String>
    <system:String x:Key="{x:Static local:ResourceKeys.EnUsKey}">en-US</system:String>

</ResourceDictionary>
