<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:resources="clr-namespace:SampleApplication.Resources">
    
    <system:String x:Key="{x:Static resources:ResourceKeys.MainWindowTitleKey}">Main Window</system:String>
    <system:String x:Key="{x:Static resources:ResourceKeys.ClickMeKey}">Click me!</system:String>
    <system:String x:Key="{x:Static resources:ResourceKeys.SomeValueKey}">SomeValue</system:String>
    
</ResourceDictionary>